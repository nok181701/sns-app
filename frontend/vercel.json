{
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/api/.*",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "allowCredentials": true,
      "dest": "/api/index.js"
    },
    {
      "src": "/images/.*",
      "headers": { "Cache-Control": "public, max-age=31536000, immutable" },
      "dest": "/public/images/$1"
    },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
